{
    "files.exclude": {
        "**/.git": true,
        "**/.svn": true,
        "**/.hg": true,
        "**/CVS": true,
        "**/.DS_Store": true,
        "vendor/**": true,
        "_site/**": true,
        ".bundle/**": true,
        ".sass-cache/**": true,
        "Gemfile.lock": true,
        "third_party": true
    },
    "files.watcherExclude": {
        "**/.git/*/**": true,
        "**/**/.DS_Store": true,
        "**/.bundle/*/**": true,
        "**/vendor/*/**": true
    },
    "files.associations": {
        "array": "cpp",
        "deque": "cpp",
        "list": "cpp",
        "vector": "cpp"
    }
}
